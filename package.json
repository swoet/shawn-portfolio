{
  "name": "shawn-portfolio-system",
  "private": true,
  "scripts": {
    "dev:admin": "cd admin-dashboard && npm run dev",
    "dev:frontend": "cd portfolio-frontend && npm run dev",
    "dev": "concurrently \"npm:dev:admin\" \"npm:dev:frontend\"",
    "install:all": "npm install && cd admin-dashboard && npm install && cd ../portfolio-frontend && npm install && cd ../shared && npm install && cd ..",
    "build": "cd admin-dashboard && npm run build && cd ../portfolio-frontend && npm run build && cd ..",
    "format": "prettier -w .",
    "prisma:generate": "prisma generate --schema=./prisma/schema.prisma",
    "prisma:migrate": "prisma migrate dev --schema=./prisma/schema.prisma",
    "prisma:studio": "prisma studio --schema=./prisma/schema.prisma",
    "prisma:seed": "tsx prisma/seed-sections.ts",
    "db:setup": "npm run prisma:generate && npm run prisma:migrate && npm run prisma:seed"
  },
  "devDependencies": {
    "concurrently": "^9.2.1",
    "prettier": "^3.6.2",
    "prisma": "^6.17.1",
    "tsx": "^4.19.2",
    "typescript": "^5.9.3"
  },
  "packageManager": "npm@10.9.0",
  "dependencies": {
    "@next-auth/prisma-adapter": "^1.0.7",
    "@prisma/client": "^6.17.1",
    "@rive-app/react-canvas": "^4.23.4",
    "@rive-app/react-webgl": "^4.23.4",
    "@splinetool/react-spline": "^4.1.0",
    "@tanstack/react-query": "^5.90.2",
    "@uploadthing/react": "^7.3.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "framer-motion": "^12.23.24",
    "lottie-react": "^2.4.1",
    "lucide-react": "^0.545.0",
    "next-auth": "^4.24.11",
    "tailwind-merge": "^3.3.1",
    "three": "^0.180.0"
  }
}
