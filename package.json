{
  "name": "shawn-portfolio-system",
  "private": true,
  "workspaces": [
    "admin-dashboard",
    "portfolio-frontend",
    "shared"
  ],
  "scripts": {
    "dev:admin": "npm --workspace admin-dashboard run dev",
    "dev:frontend": "npm --workspace portfolio-frontend run dev",
    "dev": "concurrently \"npm:dev:admin\" \"npm:dev:frontend\"",
    "build": "npm -ws run build",
    "lint": "npm -ws run lint",
    "format": "prettier -w .",
    "prisma:generate": "prisma generate --schema=./admin-dashboard/prisma/schema.prisma",
    "prisma:migrate": "prisma migrate dev --schema=./admin-dashboard/prisma/schema.prisma",
    "prisma:studio": "prisma studio --schema=./admin-dashboard/prisma/schema.prisma"
  },
  "devDependencies": {
    "concurrently": "^9.0.0",
    "prettier": "^3.3.2",
    "prisma": "6.17.1",
    "typescript": "^5.6.3"
  },
  "packageManager": "npm@10.9.0",
  "dependencies": {
    "@next-auth/prisma-adapter": "^1.0.7",
    "@prisma/client": "^6.17.1",
    "@rive-app/react-canvas": "^4.23.4",
    "@rive-app/react-webgl": "^4.23.4",
    "@splinetool/react-spline": "^4.1.0",
    "@tanstack/react-query": "^5.90.2",
    "@uploadthing/react": "^7.3.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "framer-motion": "^12.23.24",
    "lottie-react": "^2.4.1",
    "lucide-react": "^0.545.0",
    "next-auth": "^4.24.11",
    "tailwind-merge": "^3.3.1",
    "three": "^0.180.0"
  }
}
